{
	"manifest_version" : 2,
	"name" : "TMHelper",
	"short_name" : "TmH",
	"version" : "1.0",
	"author" : "markik",
	"description" : "it help you buy and sell items",
	
	"icons" : {
		"16" : "icons/16x16.png",
		"32" : "icons/32x32.png",
		"48" : "icons/48x48.png",
		"64" : "icons/64x64.png",
		"128" : "icons/128x128.png"
	},
	
	"background": {
    	"page": "background.html"
  	},

	"permissions" : [
		"https://market.dota2.net/api/*",
		"https://csgo.tm/api/*",
		"http://api.steampowered.com/*",
		"https://steamcommunity.com/*"
	],
	
    "browser_action": {
        "default_title": "settings",
        "default_icon": "icons/48x48.png",
		"default_popup": "popup.html"
    },
    
    "options_page": "options.html"
    
    
}


